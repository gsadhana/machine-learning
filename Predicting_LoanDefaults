     "execution_count": 191,
     "metadata": {},
     "output_type": "execute_result"
    },
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1152x432 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "def group_by_age(df):\n",
    "    result = df \n",
    "    \n",
    "    result['AGE'] = 2021 - result['DATE_OF_BIRTH'].dt.year \n",
    "    \n",
    "    bins  = pd.cut(df['AGE'], np.arange(20, 90, 10))\n",
    "    \n",
    "    result = result.groupby(bins)[['AGE', 'LOAN_DEFAULT', 'DISBURSED_AMOUNT']] \\\n",
    "        .agg({\n",
    "            'AGE': 'count',\n",
    "            'LOAN_DEFAULT': 'sum', \n",
    "            'DISBURSED_AMOUNT': 'mean'\n",
    "        })\n",
    "    \n",
    "    result.columns = ['TOTAL_COUNT', 'DEFAULT_COUNT', 'AVG_DISBURSED_AMOUNT']\n",
    "    result = result.reset_index()\n",
    "    \n",
    "    result['RISK_RATIO'] = result['DEFAULT_COUNT'] / result['TOTAL_COUNT']\n",
    "    \n",
    "    return result \n",
    "\n",
    "by_age = group_by_age(df)\n",
    "\n",
    "display(by_age)\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(16,6))\n",
    "\n",
    "sns.barplot(ax=ax, x='AGE', y='RISK_RATIO', data=by_age)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Looks like there isn't a huge difference in risk level based on age. Onwards!"
   ]
  },
