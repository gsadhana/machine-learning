     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1152x432 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# https://matplotlib.org/tutorials/text/annotations.html#basic-annotation\n",
    "\n",
    "def group_by_asset_cost(df):\n",
    "    global_default_rate = len(df[df['LOAN_DEFAULT'] == 1])/len(df)\n",
    "    bins=pd.cut(df['ASSET_COST'], np.arange(0, 500000, 50000))\n",
    "\n",
    "    result = df.groupby(bins)[['ASSET_COST', 'LOAN_DEFAULT']] \\\n",
    "        .agg({\n",
    "            'ASSET_COST': 'count', \n",
    "            'LOAN_DEFAULT': 'sum'\n",
    "        }) \n",
    "    \n",
    "    result.columns = ['TOTAL_COUNT', 'DEFAULT_COUNT']\n",
    "    result = result.reset_index()\n",
    "    \n",
    "    result['RISK_RATIO'] = result['DEFAULT_COUNT'] / result['TOTAL_COUNT']\n",
    "\n",
    "    return (global_default_rate, result)\n",
    "\n",
    "global_default_rate, stats_by_asset_cost = group_by_asset_cost(df)\n",
    "\n",
    "display(stats_by_asset_cost)\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(16,6))\n",
    "\n",
    "sns.barplot(x='ASSET_COST', y='RISK_RATIO', data=stats_by_asset_cost)\n",
    "ax.axhline(global_default_rate, ls='--', color='#003049')\n",
    "\n",
    "ax.text(6, global_default_rate + 0.007, 'Global Default Rate')\n",
    "\n",
    "for item in ax.get_xticklabels():\n",
    "    item.set_rotation(90)"
   ]
  },
